{
	"name": "jweiland/glossary2",
	"type": "typo3-cms-extension",
	"description": "This TYPO3 extension creates a glossary with A-Z links for you",
	"license": "GPL-2.0-or-later",
	"keywords": ["typo3", "TYPO3 CMS", "glossar", "glossary2"],
	"homepage": "http://www.jweiland.net",
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "sfroemken@jweiland.net",
			"role": "Lead Developer"
		}
	],
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/glossary2/issues",
		"source": "https://github.com/jweiland-net/glossary2"
	},
	"require": {
		"typo3/cms-core": "^9.5.17 || ^10.4.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.0",
		"nimut/testing-framework": "^5.0"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\Glossary2\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\Events2\\Tests\\": "Tests"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "glossary2",
			"web-dir": ".Build/public"
		}
	}
}
